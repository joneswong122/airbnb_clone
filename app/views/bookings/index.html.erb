<div class="container pt-3">
<h1>Bookings list</h1>

<br>

<% @bookings.each do |booking| %>
  <%= booking.flat.title %>
  <br>
  <%= booking.flat.address %>
  <br>
  <%= booking.flat.city %>
  <br>
  <%= booking.start_time %>
  <br>
  <%= booking.end_time %>
  <br>
  <%= booking.user.email %>
  <hr>
  <%= link_to "visit", flat_path(booking.flat)  %><br>
  <% link_to "info", booking_path(booking)%><br>
<% end %>
</div>
